<!-- Gallery Page -->
<% console.log("Gallery Page: "+ page.path); 
 var photos=getGalleryPhotoNames(path); 
 console.log("return photos: "+ photos);
%>
<script src="https://apps.bdimg.com/libs/jquery/2.1.4/jquery.min.js">
</script>
<script type="text/javascript">
  console.log("li>a.naav__menu_li#gallery");
  $("li>a.nav__menu_li#gallery").addClass("menu--active")
</script>

<div class="gallery__page">
  <div class="gallery__warp">
    <div class="row">
      <% for (let path of photos){ %>
      <% var raw_link=url_for("/gallery/imgs/"+path) %>
      <% var thumbnail_link=url_for("/gallery/thumbnails/"+path) %>

      <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
        <a class="gallery__item fresco" href="<%= raw_link %> " data-fresco-group="gallery">
          <img src="<%= thumbnail_link %>" alt="">
        </a>
      </div>
      <%} %>

    </div>
  </div>
</div>
<!-- Gallery Page end -->