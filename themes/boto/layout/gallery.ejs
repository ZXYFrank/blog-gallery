<!-- Gallery Page -->
<% console.log("Gallery Page: "+ page.path); 
 var photos=get_gallery_photo_names(path); 
 console.log("return photos: "+ photos);
%>

<div class="gallery__page">
  <div class="gallery__warp">
    <div class="row">
      <% for (let path of photos){ %>
      <% var link=url_for("/gallery/imgs/"+path) %>

      <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
        <a class="gallery__item fresco" href="<%= link %> " data-fresco-group="gallery">
          <img src="<%= link %>" alt="">
        </a>
      </div>
      <%} %>

    </div>
  </div>
</div>
<!-- Gallery Page end -->